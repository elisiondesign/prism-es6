{
  "name": "prism-es6",
  "version": "1.2.0",
  "description": "Enable prismjs library to be imported using ES6 syntax",
  "main": "prism.js",
  "scripts": {
    "build": "npm run clean && npm run cp:themes && npm run cp:components",
    "clean": "npx rimraf ./themes && npx rimraf ./components",
    "cp:themes": "cp -r ./node_modules/prismjs/themes ./themes",
    "cp:components": "cp -r ./node_modules/prismjs/components ./components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elisiondesign/prism-es6.git"
  },
  "keywords": [
    "prismjs",
    "es6",
    "wrapper"
  ],
  "author": "Jan BÃ­lek",
